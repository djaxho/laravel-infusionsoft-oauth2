{
    "name": "djaxho/laravel-infusionsoft-oauth2",
    "type": "library",
    "description": "Laravel package to facilitate the use of the Infusionsoft API using OAUTH2",
    "keywords": [
        "djaxho",
        "laravel-infusionsoft-oauth2",
        "laravel",
        "infusionsoft",
        "oauth2"
    ],
    "homepage": "https://github.com/djaxho/laravel-infusionsoft-oauth2",
    "license": "MIT",
    "authors": [
        {
            "name": "Danny Jackson",
            "email": "djaxho@gmail.com",
            "homepage": "http://www.premiumwaves.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/support": "^5.4",
        "infusionsoft/php-sdk": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Djaxho\\LaravelInfusionsoftOauth2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Djaxho\\LaravelInfusionsoftOauth2\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
